<?xml version="1.0" encoding="utf-8" standalone="no"?>
<?xml-stylesheet type="text/xsl" href="#sc_stylesheet"?>
<!DOCTYPE supportchart [<!ATTLIST xsl:stylesheet id ID #REQUIRED>]>

<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" id="sc_stylesheet" version="1.0">
	<xsl:output method="html" omit-xml-declaration="yes"/>
	<xsl:variable name="support_chart">
		<supportchart>
			 
			<Table CreatedOn="Tue Aug 19 08:51:32 2025" Name="OS Support Chart Mapping" SDKVersion="4.3.4314.0">
				<Version Name="Mac OS X">
					<Edition Name="Mac OS X Puma" OsId="1001"/>
					<Edition Name="Mac OS X Jaguar" OsId="1002"/>
					<Edition Name="Mac OS X Panther" OsId="1003"/>
					<Edition Name="Mac OS X Tiger" OsId="1004"/>
					<Edition Name="Mac OS X Leopard" OsId="1005"/>
					<Edition Name="Mac OS X Snow Leopard" OsId="1006"/>
				</Version>
				<Version Name="OS X">
					<Edition Name="Mac OS X Lion" OsId="1007"/>
					<Edition Name="OS X Mountain Lion" OsId="1008"/>
					<Edition Name="OS X Mavericks" OsId="1009"/>
					<Edition Name="OS X Yosemite" OsId="1010"/>
					<Edition Name="OS X El Capitan" OsId="1011"/>
					<Edition Name="macOS Sierra" OsId="1012"/>
					<Edition Name="macOS High Sierra" OsId="1013"/>
					<Edition Name="macOS Mojave" OsId="1014"/>
					<Edition Name="macOS Catalina" OsId="1015"/>
				</Version>
				<Version Name="macOS ">
					<Edition Name="macOS Big Sur" OsId="1100"/>
					<Edition Name="macOS Monterey" OsId="1200"/>
					<Edition Name="macOS Ventura" OsId="1300"/>
					<Edition Name="macOS Sonoma" OsId="1400"/>
					<Edition Name="macOS Sequoia" OsId="1500"/>
					<Edition Name="macOS Tahoe" OsId="1501"/>
				</Version>
			</Table>
		</supportchart>
	</xsl:variable>
	<xsl:template match="Table">
		<html>
			<head>
				<meta content="text/html; charset=utf-8" http-equiv="Content-Type"/>
				<title>Support Chart - OS IDs</title>
				<style media="screen" type="text/css">
					/*
					CSS File for support charts.

					Modify styles here
					*/
							
					A:visited 
					{
					    color: Black;
					    text-decoration: none;
					}

					A:link
					{
					    color: Black;
					    text-decoration:none;
					}
					A:hover
					{
					    color: blue;
					}
							
					table	        {border-collapse: collapse;}

					th            {border: 1px solid black; color: #0000ff; font-weight: bold; background: #FFFFAA; margin:0; padding:0;text-align: center;}
					th.vert       {writing-mode: tb-rl; filter: flipv fliph; width: 25px; height: 125px; }
					th.span    {border: 1px solid black; color: #0000ff; font-weight: bold; background: #999999;}

					td {
					    font-size: 12px; 
					    font-family: Arial; 
					    border: 1px solid gray; 
					    padding: 2px 5px 2px 5px;
					}
					td.multicolumn {
						padding: 0px 0px 0px 0px;
						margin: 0px 0px 0px 0px;
						border: 0px solid gray;
					}
					table.multicolumn {
					 margin: 0;
					 padding: 0 0 0 0;
					 border-collapse: collapse;
					 height: 100%;
					  }
					
					th.wrap_v_text{
						display: block-inline;
						height: 165px;
						width: 25px;
						padding: 0px 0px 3px 2px;
					}
					div.vtext {
						display: block-inline;;
						width: 20px;
						white-space: nowrap;
						padding: 0px;

						font-family: Arial;
						font-weight: heavy;

						-webkit-transform: rotate(-90deg);
						-moz-transform: rotate(-90deg);
						-ms-transform: rotate(-90deg);
						-o-transform: rotate(-90deg);
						transform: rotate(-90deg);

						-webkit-transform-origin: right bottom 0px;
						-moz-transform-origin: right bottom 0px;
						-ms-transform-origin: right bottom 0px;
						-o-transform-origin: right bottom 0px;
						transform-origin: right bottom 0px;

						/* This is needed because transformations result in overlap with the row below the header labels, same length as the width of this div */
						margin-bottom: 20px;
					}

					#modtext_medium{
						font-size: 0.9em;
					}

					#modtext_small{
						font-size: 0.8em;
					}

					#modtext_tiny{
						font-size: 0.72em;
					}

					#modtext_microscopic{
						font-size: 0.65em;
					}

					th.threatlog {box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; border-bottom: #000 2px dotted; border-top: #000 2px dotted;}
					#threatright {border-right: #000 2px dotted;}
					#threatleft {border-left: #000 2px dotted;}

					td.vend       {font-size: 12px; font-family: Arial; border: 1px solid black; font-weight:bold; color: #000000; background: #cccccc; }
					td.title      {font-size: 24px; font-family: Arial; border: 0px solid black; font-weight:bold; color: #000000; background: #FFCC66; }
					td.content    {font-size: 12px; font-family: Arial; border: 0px solid black; font-weight:bold; color: #000000; background: #FFCC66; }
					td.cellv      {width: 20px; height: 15px; font-family: Arial; text-align: center; font-weight: bold; border: 1px solid black; color: #000000; background: #66FF99; }
					td.cellva     {width: 20px; height: 15px; font-family: Arial; text-align: center; font-weight: bold; border: 1px solid black; color: #000000; background: #0CEEDD; }
					td.cello      {width: 20px; height: 15px; font-family: Arial; text-align: center; font-weight: bold; border: 1px solid black; color: #000000; background: #FFFF99; }
					td.cellx      {width: 20px; height: 15px; font-family: Arial; text-align: center; font-weight: bold; border: 1px solid black; color: #000000; background: #CC9966; }
					td.cellz      {width: 20px; height: 15px; font-family: Arial; text-align: center; font-weight: bold; border: 1px solid black; color: #000000; background: #FFCCCC; }
					td.celln      {width: 20px; height: 15px; font-family: Arial; text-align: center; font-weight: bold; border: 1px solid black; color: #000000; background: #FFFFFF; }

					/*  End CSS  */
				</style>
			</head>
			<body leftmargin="0" onload="f50InitTableHeader('bcsdk');" rightmargin="0" topmargin="0">
				<table border="1" width="1600">
					<tr>
						<td class="title">
							<xsl:value-of select="@Name"/>
							/ SDK Version - 
							<xsl:value-of select="@SDKVersion"/>
						</td>
					</tr>
					<tr>
						<td>
							Created On - 
							<xsl:value-of select="@CreatedOn"/>
						</td>
					</tr>
				</table>
				<table border="1" width="1600">
					<tbody id="OS IDs">
						<tr align="left" valign="bottom">
							<th width="600">OS Name</th>
							<th width="100">OS ID</th>
							<th>Comments</th>
						</tr>
						<xsl:for-each select="Version">
							<tr>
								<td class="vend" colspan="32">
									<xsl:value-of select="@Name"/>
								</td>
							</tr>
							<xsl:for-each select="Edition">
								<tr align="left">
									<td>
										<xsl:value-of select="@Name"/>
									</td>
									<td>
										<xsl:value-of select="@OsId"/>
									</td>
									<td width="1200"/>
								</tr>
							</xsl:for-each>
						</xsl:for-each>
					</tbody>
				</table>
				<br/>
				<br/>
			</body>
		</html>
	</xsl:template>
	<xsl:template match="xsl:template"/>
</xsl:stylesheet>
